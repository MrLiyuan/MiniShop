<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>minishop</display-name>
  <welcome-file-list>
    <welcome-file>HomePage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>OneGoods</servlet-name>
    <servlet-class>team.rngu.shop.servlet.GoodsIntrouductionPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OneGoods</servlet-name>
    <url-pattern>/intro.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>buy</servlet-name>
    <servlet-class>team.rngu.shop.servlet.GoodsBuyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>buy</servlet-name>
    <url-pattern>/buy.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cart</servlet-name>
    <servlet-class>team.rngu.shop.servlet.GoodsAddCartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/addcart.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>team.rngu.shop.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userlist</servlet-name>
    <servlet-class>team.rngu.shop.servlet.BackUserListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userlist</servlet-name>
    <url-pattern>/ulist.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addGoods</servlet-name>
    <servlet-class>team.rngu.shop.servlet.AddGoodsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addGoods</servlet-name>
    <url-pattern>/addGoods.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>delindent</servlet-name>
    <servlet-class>team.rngu.shop.servlet.DeleteIndentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delindent</servlet-name>
    <url-pattern>/delindent.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deluser</servlet-name>
    <servlet-class>team.rngu.shop.servlet.DeleteUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deluser</servlet-name>
    <url-pattern>/deluser.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>delgoods</servlet-name>
    <servlet-class>team.rngu.shop.servlet.DeleteGoodsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delgoods</servlet-name>
    <url-pattern>/delgoods.action</url-pattern>
  </servlet-mapping>
</web-app>